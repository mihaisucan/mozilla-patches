-function test() {
-  addTab(TEST_URI);
-  browser.addEventListener("DOMContentLoaded", testDisplayAccessors, false);
-}
-
-function testDisplayAccessors() {
-  browser.removeEventListener("DOMContentLoaded", testDisplayAccessors,
-                              false);
